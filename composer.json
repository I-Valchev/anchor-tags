{
    "name": "ivo-valchev/anchor-tags",
    "description": "🏷 This Bolt extension adds IDs to HTML elements for use as anchor tags.",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivo Valchev",
            "email": "me@ivovalchev.com"
        }
    ],
    "require": {
        "php": ">=7.2.9",
        "twig/twig": "^2.12 | ^3.0"
    },
    "require-dev": {
        "bolt/core": "^4.1",
        "symplify/easy-coding-standard": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "IvoValchev\\AnchorTags\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "IvoValchev\\AnchorTags\\Extension"
    }
}
